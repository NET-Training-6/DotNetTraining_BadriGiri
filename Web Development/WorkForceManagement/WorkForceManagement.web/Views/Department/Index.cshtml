@model List<Department>;


<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Descriptions</th>
            <th scope="col">Established Date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach(Department d in Model)
        {
            <tr>
                <td>@d.ID</td>
                <td>@d.Name</td>
                <td>@d.Description</td>
                <td>@d.Established.ToString("MMM dd, yyyy")</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@d.ID"><i class="bi bi-pen "></i> </a>
                    <a asp-action="Delete" asp-route-id="@d.ID"><i class="bi bi-trash text-danger"></i> </a>
                </td>
            </tr>
            
        }



    </tbody>
</table>