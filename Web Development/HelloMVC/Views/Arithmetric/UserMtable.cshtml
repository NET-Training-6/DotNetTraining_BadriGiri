@model (int from,int to) 





   <div class="container text-center">
       
            <div class="row">
                @for (int i = Model.from; i < Model.to; i++)
                {
                    <div class="col-3 border-4">
                       
                        @for(int j=1;j<10;j++)
                        {
                            <li>@i X @j=@(i * j)</li>
                            <br />
                        }
                    </div>     
                }
            </div>
   </div>
@{
    for(int i=Model.from;i<Model.to;i++)
    {
        for(int j=1;j<10;j++)
        {
            
                <li>@($"{i} X {j}={i * j}")</li>
        }
        <hr />
    }
}
